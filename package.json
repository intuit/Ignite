{
  "name": "ignite",
  "version": "1.11.2",
  "description": "Modern markdown documentation generator",
  "main": "dist/ignite.js",
  "license": "Apache-2.0",
  "bin": {
    "ignite": "./dist/ignite-cli.js"
  },
  "files": [
    "dist",
    "src",
    "webpack.config.js",
    "webpack.config.dev.js",
    "markdownit.config.js",
    "*.js",
    "build-utils"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/Ignite/"
  },
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "xo",
    "lint:css": "stylelint 'src/**/*.css'",
    "test": "jest --roots src",
    "test:automation": "jest --roots automation --preset jest-puppeteer --runInBand --coverage=false",
    "serve": "webpack-serve",
    "start": "npm run build -- -w",
    "build": "chmod 777 ./dist/ignite-cli.js && ./dist/ignite-cli.js",
    "build:lib": "babel src -d dist --ignore '**/__tests__/' --copy-files",
    "build:lib:watch": "npm run build:lib -- -w --verbose",
    "publish:docs": "npm run build -- --publish",
    "analyze": "npm run build -- --analyze",
    "release": "auto shipit",
    "add-contributor": "all-contributors add",
    "version:canary": "auto canary"
  },
  "dependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-3": "7.0.0",
    "@babel/register": "7.0.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "8.0.4",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.21",
    "bulma-checkradio": "2.1.0",
    "cheerio": "1.0.0-rc.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "1.0.0",
    "connect-history-api-fallback": "1.5.0",
    "copy-webpack-plugin": "4.6.0",
    "cosmiconfig": "5.0.7",
    "cpy": "7.0.1",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "dayjs": "1.7.5",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "get-line-from-pos": "1.0.0",
    "gh-pages": "^2.0.1",
    "globby": "^8.0.1",
    "gravatar": "1.8.0",
    "highlight.js": "^10.1.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "4.6.0",
    "js-worker-search": "1.4.0",
    "koa-connect": "2.0.1",
    "loader-utils": "^1.1.0",
    "lqip-loader": "2.2.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-attrs": "2.3.2",
    "markdown-it-br": "^1.0.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-front-matter": "git+https://github.com/hipstersmoothie/markdown-it-front-matter.git#use-callback",
    "markdown-it-highlight-lines": "^1.0.2",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-jsx": "1.1.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "0.4.3",
    "markdown-it-vanilla-loader": "1.0.10",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "0.5.1",
    "opn": "^5.4.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.1",
    "postcss-simple-vars": "^5.0.1",
    "pretty-exceptions": "^1.4.0",
    "probe-image-size": "4.0.0",
    "prop-types": "^15.6.2",
    "puppeteer": "1.8.0",
    "react": "^16.6.3",
    "react-color": "2.14.1",
    "react-dom": "^16.6.3",
    "react-gist": "1.2.1",
    "react-ideal-image": "0.0.5",
    "react-router-prop-types": "^1.0.4",
    "react-tweet-embed": "1.1.1",
    "react-waypoint": "8.1.0",
    "request": "^2.88.0",
    "request-promise": "4.2.2",
    "responsive-loader": "1.1.0",
    "root-path": "^0.2.1",
    "serve-handler": "5.0.7",
    "simple-git": "1.107.0",
    "style-loader": "^0.23.1",
    "throttle-debounce": "2.0.1",
    "transform-markdown-links": "^2.0.0",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cdn-plugin": "2.3.3",
    "webpack-inject-plugin": "~1.0.2",
    "webpack-merge": "^4.1.5",
    "webpack-serve": "2.0.3",
    "webpack-serve-waitpage": "^1.0.2",
    "yaml": "1.1.0",
    "zeedown": "2.0.2"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.1",
    "auto": "^5.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "jest-css-modules-transform": "2.1.1",
    "jest-junit": "^5.2.0",
    "jest-puppeteer": "^3.3.1",
    "jest-watch-typeahead": "0.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.6.3",
    "react-testing-library": "5.3.2",
    "snapshot-diff": "0.4.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-csstree-validator": "1.3.0",
    "stylelint-scss": "^3.4.1",
    "xo": "^0.23.0",
    "yargs": "^12.0.5"
  },
  "resolutions": {
    "lqip": "github:hipstersmoothie/lqip#5bea04ce952ddd2d82a0a06fe1efb4df723f1e41"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "prettier",
      "xo-react/space"
    ],
    "plugins": [
      "prettier",
      "jest"
    ],
    "env": [
      "browser",
      "node",
      "jest"
    ],
    "rules": {
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "max-params": [
        1,
        5
      ],
      "react/jsx-tag-spacing": {
        "closingSlash": "never",
        "beforeSelfClosing": "always",
        "afterOpening": "never"
      }
    }
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "\\.(css)$": "jest-css-modules-transform",
      "^.+\\.js?$": "babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/snapshot-diff/serializer.js"
    ],
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/",
      "/__tests__/utils/",
      "/testExtension/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/testExtension/*.js",
      "!**/create-static-site.js"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "lcov"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "jest-junit": {
    "output": "./coverage/junit/junit.xml"
  }
}
